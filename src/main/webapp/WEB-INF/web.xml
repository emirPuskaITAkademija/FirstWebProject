<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <servlet>
        <servlet-name>FirstServlet</servlet-name>
        <servlet-class>com.solution.best.app.dynamic.FirstServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SabiraServlet</servlet-name>
        <servlet-class>com.solution.best.app.dynamic.SabiraServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FirstServlet</servlet-name>
        <url-pattern>/prvi</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SabiraServlet</servlet-name>
        <url-pattern>/sabira</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>welcome.html</welcome-file>
    </welcome-file-list>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
